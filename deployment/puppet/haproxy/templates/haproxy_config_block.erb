
listen <%= name %>  <% virtual_ips do |virtual_ip| %> <%= virtual_ip %>:<%= virtual_ip_port %>  <% end -%>
<% haproxy_config_options.sort.each do |key, val| -%>
<% if val.is_a?(Array) -%>
<% val.each do |item| -%>
  <%= key %>  <%= item %>
<% end -%>
<% else -%>
  <%= key %>  <%= val %>
<% end -%>
<% end -%>
